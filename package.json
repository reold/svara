{
  "name": "svara",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:ghp": "vite build --mode ghp && touch dist/.nojekyll",
    "build:cfp": "vite build --mode cfp",
    "build": "run-p build:ghp build:cfp",
    "clean-cache": "rm -rf node_modules/.cache/gh-pages",
    "deploy:ghp": "gh-pages -d dist --dotfiles -f",
    "deploy:cfp": "gh-pages -d cfp-dist -b cf-pages -f",
    "deploy": "run-s clean-cache && run-p deploy:ghp deploy:cfp",
    "bandd": "run-s build deploy",
    "preview": "vite preview --base /svara/",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/postcss": "^4.1.6",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node-forge": "^1.3.11",
    "gh-pages": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "svelte": "^5.1.3",
    "svelte-check": "^4.0.5",
    "tailwindcss": "^4.1.6",
    "tslib": "^2.8.0",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-pwa": "^1.0.0"
  },
  "dependencies": {
    "colorthief": "^2.6.0",
    "node-forge": "^1.3.1",
    "peerjs": "^1.5.4",
    "reold-opfs": "gist:86a64385efd4ec10674f4744da655505"
  }
}
